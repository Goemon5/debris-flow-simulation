FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      scalarTransportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// 匂い物質の拡散係数（空気中のガス拡散）
DT              2.0e-05;  // [m^2/s] 分子拡散係数

// Schmidt数（乱流拡散の設定）
Sct             0.7;      // 乱流Schmidt数

