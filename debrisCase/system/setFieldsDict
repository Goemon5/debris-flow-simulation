FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue s 0  // 全領域で濃度0
);

regions
(
    cellToCell
    {
        set odorSourceCells;  // topoSetで定義した領域
        fieldValues
        (
            volScalarFieldValue s 1.0  // 匂い源で濃度1
        );
    }
);

// ************************************************************************* //
